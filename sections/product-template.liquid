                              <div style="position: absolute; left: -5000px;" aria-hidden="true"><input type="text" id="mchimpoos_code" name="b_a16a4d2f27faf82c295324f89_{mchimpcode}" tabindex="-1" value=""></div>
                              <div class="actions">
                                  <button class="btn ladda-button" name="subscribe" title="Get Notified" type="submit">
                                      <span class="ladda-label">Get Notified</span>
                                  </button>
                              </div>
                          </div>
                      </div>
              </form>
          </div>
          {% endif %}
          {%endcomment%}
          {% unless section.settings.show_extra_tab == false or pages[section.settings.extra_tab_content] == empty %}
            <div class="tabs">
              <ul class="tab-switch__nav">
                <li>
                  <a href="#description" data-link="description" class="tab-switch__trigger h3">{{ 'products.product.description' | t }}</a>
                </li>
                <li>
                  <a href="#extra" data-link="extra" class="tab-switch__trigger h3">{{ pages[section.settings.extra_tab_content].title }}</a>
                </li>
              </ul>
              <div id="description" class="tab-switch__content" data-content="description">
                <div class="product-description rte" itemprop="description">
                  {{ product.description }}
                </div>
              </div>
              <div id="extra" class="tab-switch__content" data-content="extra">
                <div class="rte">
                  {{ pages[section.settings.extra_tab_content].content }}
                </div>
              </div>
            </div>
          {% else %}
            <div class="grid__item post-large--four-fifths medium-down--one-whole mx-auto product-short-description rte pl-sm-0" itemprop="short-description">
              {{ product.metafields.global.short_desc }}
            </div>
            <div class="grid__item post-large--four-fifths medium-down--one-whole mx-auto product-description rte pl-sm-0" itemprop="description">
              {{ product.description }}
            </div>
          {% endunless %}
          {% if section.settings.social_sharing %}
          <div class="post-large--one-quarter mx-auto product-sharing-box text-center">
            <h2 class="h4 tk-futura-pt">{{ 'products.general.share_title' | t }}</h2>
            {% include 'social-sharing' %}
            </div>
          {% endif %}
