              <span id="AddToCartText">{{ 'products.product.add_to_cart' | t }}</span>
            </button>
            <a id="bisCta" href="#" class="btn btn-prive btn--secondary bis-button BIS_trigger">Notify me when available</a>
            {% if product.tags contains 'RxAble'%}
            <a id="VisionWebRX" href="#" class="btn btn-prive btn--secondary btn-rx"><span id="RxFormulaText">Add prescription</span></a>
            {% endif %}         
            {% if section.settings.enable_payment_button%}
              <div class="other-payments" {% if variant.inventory_quantity  <= 0 %}style="display:none;"{%endif%}>
              {{ form | payment_button }}
              </div>
            {% endif %}
            </div>
          {% endform %}
          </div>
          {%comment%}
          {% if one_is_not_available == true %}    
          <div class="grid__item post-large--one-whole oos-wrapper" id="oos">
              <form class="form outofstock subscribe" target="_blank" novalidate="novalidate" action="//priverevaux.us14.list-manage.com/subscribe/post?u=a16a4d2f27faf82c295324f89&amp;id={mchimpcode}" method="post" id="newsletter-validate-detail">
                  <div class="grid__item post-large--two-fifths">
                          <h2 class="tk-big-caslon-fb">Sold Out!</h2>
